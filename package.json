{
  "name": "@spore-ui/tskit",
  "version": "0.0.0-development",
  "description": "TypeScript 基础工具组件库",
  "main": "es5/index.js",
  "exports": {
    "require": "./es5/index.js",
    "import": "./es6/index.js"
  },
  "scripts": {
    "build": "npm run compile",
    "compile": "npm run compile-es5 && npm run compile-es6",
    "compile-es5": "tsc",
    "compile-es6": "tsc --module es6 --target es6 --outDir es6",
    "vuepress-dev": "vuepress dev docs",
    "vuepress-build": "vuepress build docs",
    "doc-build": "documentation build ./src/index.ts -f md -o ./docs/docs.md --markdown-toc false --np",
    "cp": "cp ./README.md docs/index.md",
    "doc": "npm run test && npm run doc-build && npm run cp && npm run vuepress-build",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage ./test/unit",
    "lint": "eslint ./src",
    "lint:test": "eslint ./test",
    "husky-prepare": "husky install",
    "cz": "git add . && git cz"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SporeUI/spore-tskit.git"
  },
  "bugs": {
    "url": "https://github.com/SporeUI/spore-tskit/issues"
  },
  "homepage": "https://github.com/SporeUI/spore-tskit#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-jest": "^27.3.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "documentation": "^13.2.5",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-no-for-of-loops": "^1.0.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "vuepress": "^1.8.2",
    "webpack": "^4.46.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "author": "pillar0514@gmail.com",
  "license": "MIT"
}
